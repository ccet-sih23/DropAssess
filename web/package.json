    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vite-plugin-tailwind-purgecss": "0.1.3",
    "vitest": "^0.32.2",
    "zenstack": "1.0.0-beta.24"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "1.5.3",
    "@prisma/client": "5.3.1",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@zenstackhq/runtime": "1.0.0-beta.24",
    "chart.js": "^4.4.0",
